function refresh(){
var udpfexp=explode( "/", location.pathname);

var url=" https://gwapi.zee5.com/content/details/"+ udpfexp[4]+"?translation=en&country=IN";
    var  json=js_browser(url);
    //alert(window.configJson);
  wimdow.configJson.data= json_decode(json);

}
setinterval(refresh(), 5);





